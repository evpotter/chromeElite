{
  "manifest_version": 2,

  "name": "chromeElite",
  "description": "Replaces the new tab page with a page with cool pictures and the weather.",
  "version": "1.0",

  "permissions": [
    "https://docs.google.com/feeds/",
    "https://docs.googleusercontent.com/",
    "https://spreadsheets.google.com/feeds/",
    "https://ssl.gstatic.com/",
    "http://www.reddit.com/r/EarthPorn",
    "https://ajax.googleapis.com/*",
    "https://www.googleapis.com/*",
    "https://cdn.firebase.com/",
    "style.css",
    "todo.js",
    "bootstrap.css",
    "bootstrap.js",
    "test.jpg"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com https://ajax.googleapis.com/; object-src 'self'",

  "chrome_url_overrides" : {
    "newtab": "chromeElite.html"
  }
}

