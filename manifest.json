{
  "manifest_version": 2,

  "name": "chromeElite",
  "description": "Replaces the new tab page with a page with cool pictures and the weather.",
  "version": "1.0",

  "permissions": [
   "http://www.reddit.com/r/pics",
   "https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"
  ],
  
  "chrome_url_overrides" : {
    "newtab": "chromeElite.html"
  }
}

