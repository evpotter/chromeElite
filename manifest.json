{
  "manifest_version": 2,

  "name": "chromeElite",
  "description": "Replaces the new tab page with a page with cool pictures and the weather.",
  "version": "1.0",

  "permissions": [
   "http://www.reddit.com/r/EarthPorn",
   "https://ajax.googleapis.com/ajax/libs/jquery/*",
   "https://www.google.com/*",
   "https://www.gmodules.com/*",
   "http://gmodules.com/*",
   "style.css",
   "http://*/*",
   "https://*/*",
   "test.jpg",
   "calendar.html"
  ],
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["jquery.min.js","functions.js"],
      "run_at" : "document_end",
      "all_frames" : false
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google.com/*; object-src 'self'",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "chromeElite.html"
  }
}

